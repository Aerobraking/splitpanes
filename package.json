{
  "name": "splitpanes",
  "version": "1.0.1",
  "description": "A Vue JS reliable and simple panes splitter / resizer",
  "repository": "https://github.com/antoniandre/splitpanes",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "license": "MIT",
  "main": "./dist/splitpanes.common.js",
  "unpkg": "dist/splitpanes.umd.min.js",
  "keywords": [
    "split panes",
    "panes resizer",
    "vue",
    "vuejs",
    "ui"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name splitpanes ./src/components/splitpanes.vue --dest ./dist"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-rc.1",
    "material-design-icons-iconfont": "^3.0.3",
    "roboto-fontface": "*",
    "vue": "^2.5.17",
    "vuetify": "^1.2.7"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.4.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "node-sass": "^4.9.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "simple-syntax-highlighter": "^1.0.0",
    "vue-cli-plugin-vuetify": "^0.2.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
